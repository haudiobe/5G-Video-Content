{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "https://raw.githubusercontent.com/haudiobe/5G-Video-Content/main/3gpp-raw-schema.json",
  "type": "object",
  "title": "3GPP 5G Video Raw Sequence Format",
  "description": "Provides the raw 3GPP 5G Video Raw Sequence Format.",
  "default": {},
  "examples": [
    {
      "Sequence": {
        "Name": "Sequence",
        "Background": "The Sequence is generated by someone and was provided by the study and shows a 3GPP meeting.",
        "Scenario": "FullHD",
        "Key": "S1-R1",
        "URI": "./sequence.yuv",
        "md5": "824fe384be7b0095c391ff6c5a5acc53",
        "thumbnail": "./sequence.jpg",
        "preview": "./sequence.mp4"
      },
      "Properties": {
        "width": 1280,
        "height": 720,
        "format": "yuv",
        "packing": "interleaved",
        "scan": "progressive",
        "subsampling": "420",
        "bitDepth": 8,
        "frameRate": 30,
        "colourPrimaries": 1,
        "transferCharacteristics": 1,
        "matrixCoefficients": 1,
        "sampleAspectRatio": 1,
        "duration": 10,
        "frameCount": 30
      },
      "copyRight": "This material has been recorded by the Thomas Stockhammer, who owns the copyright. Thomas Stockhammer (licensor) has licensed it under the Creative Commons Attribution 4.0 license (https://creativecommons.org/licenses/by/4.0/). In short, this means you can freely reuse and distribute this content, also commercially, for as long you provide a proper attribution.",
      "Contact": {
        "Name": "Thomas Stockhammer",
        "Company": "Qualcomm",
        "e-mail": "tsto@qti.qualcomm.com"
      }
    }
  ],
  "required": [
    "Sequence",
    "Properties",
    "copyRight"
  ],
  "properties": {
    "Sequence": {
      "$id": "#/properties/Sequence",
      "type": "object",
      "title": "Sequence",
      "description": "provides the details of the Sequence.",
      "default": {},
      "examples": [
        {
          "Name": "Sequence",
          "Background": "The Sequence is generated by someone and was provided by the study and shows a 3GPP meeting.",
          "Scenario": "FullHD",
          "Key": "S1-R1",
          "URI": "./sequence.yuv",
          "md5": "824fe384be7b0095c391ff6c5a5acc53",
          "thumbnail": "./sequence.jpg",
          "preview": "./sequence.mp4"
        }
      ],
      "required": [
        "Name",
        "URI",
        "md5"
      ],
      "properties": {
        "Name": {
          "$id": "#/properties/Sequence/properties/Name",
          "type": "string",
          "title": "Name",
          "description": "The name of the sequence.",
          "default": "",
          "examples": [
            "Sequence"
          ]
        },
        "Scenario": {
          "$id": "#/properties/Sequence/properties/Scenario",
          "type": "string",
          "title": "Scenario",
          "description": "The Scenario for which the sequence applies.",
          "enum": [
            "FullHD",
            "4KTV",
            "Messaging",
            "Gaming",
            "Screen",
            "multiple",
            "unknown"
          ],
          "default": ""
        },
        "Key": {
          "$id": "#/properties/Sequence/properties/Key",
          "type": "string",
          "title": "Key",
          "description": "The key of the sequence in TR 26.955.",
          "default": "",
          "examples": [
            "S1-R1"
          ]
        },
        "Background": {
          "$id": "#/properties/Sequence/properties/Background",
          "type": "string",
          "title": "Background",
          "description": "Provides background how the sequence was generated and what one can see.",
          "default": "",
          "examples": [
            "The Sequence is generated by someone and was provided by the study and shows a 3GPP meeting."
          ]
        },
        "URI": {
          "$id": "#/properties/Sequence/properties/URI",
          "type": "string",
          "format": "uri-reference",
          "title": "URI",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "./sequence.yuv"
          ]
        },
        "md5": {
          "$id": "#/properties/Sequence/properties/md5",
          "type": "string",
          "title": "MD5",
          "description": "MD5 of the sequence file according to RFC1321.",
          "default": "",
          "examples": [
            "824fe384be7b0095c391ff6c5a5acc53"
          ]
        },
        "thumbnail": {
          "$id": "#/properties/Sequence/properties/thumbnail",
          "type": "string",
          "format": "uri-reference",
          "title": "Thumbnail image",
          "description": "An image providing a thumbnail.",
          "default": "",
          "examples": [
            "./sequence.jpg"
          ]
        },
        "preview": {
          "$id": "#/properties/Sequence/properties/preview",
          "type": "string",
          "format": "uri-reference",
          "title": "MP4 preview",
          "description": "An mp4 providing a preview.",
          "default": "",
          "examples": [
            "./sequence.mp4"
          ]
        }
      },
      "additionalProperties": true
    },
    "Properties": {
      "$id": "#/properties/Properties",
      "type": "object",
      "title": "Properties",
      "description": "An explanation about the purpose of this instance.",
      "default": {},
      "examples": [
        {
          "width": 1280,
          "height": 720,
          "format": "yuv",
          "packing": "interleaved",
          "scan": "progressive",
          "subsampling": "420",
          "bitDepth": 8,
          "frameRate": 30,
          "colourPrimaries": 1,
          "transferCharacteristics": 1,
          "matrixCoefficients": 1,
          "sampleAspectRatio": 1,
          "duration": 10,
          "frameCount": 30
        }
      ],
      "required": [
        "width",
        "height",
        "format",
        "packing",
        "scan",
        "subsampling",
        "bitDepth",
        "frameRate",
        "colourPrimaries",
        "transferCharacteristics",
        "matrixCoefficients",
        "frameCount"
      ],
      "properties": {
        "width": {
          "$id": "#/properties/Properties/properties/width",
          "type": "integer",
          "title": "Width",
          "description": "Width of first component in number of pixel",
          "default": 1920,
          "examples": [
            3840,
            1920,
            1280
          ]
        },
        "height": {
          "$id": "#/properties/Properties/properties/height",
          "type": "integer",
          "title": "Height",
          "description": "Height of first component in number of pixel.",
          "default": 1080,
          "examples": [
            2160,
            1080,
            720
          ]
        },
        "format": {
          "$id": "#/properties/Properties/properties/format",
          "type": "string",
          "enum": [
            "yuv",
            "rgb"
          ],
          "title": "The format schema",
          "description": "Format of the components",
          "default": "yuv"
        },
        "packing": {
          "$id": "#/properties/Properties/properties/packing",
          "type": "string",
          "enum": [
            "interleaved",
            "planar"
          ],
          "title": "Component packing",
          "description": "How is the component packed.",
          "default": "planar"
        },
        "scan": {
          "$id": "#/properties/Properties/properties/scan",
          "type": "string",
          "enum": [
            "progressive",
            "interlace"
          ],
          "title": "Scan type",
          "description": "What is is the scan type.",
          "default": "progressive"
        },
        "subsampling": {
          "$id": "#/properties/Properties/properties/subsampling",
          "type": "string",
          "enum": [
            "420",
            "422",
            "444"
          ],
          "title": "Subsampling pattern",
          "description": "An explanation about the purpose of this instance.",
          "default": "420"
        },
        "bitDepth": {
          "$id": "#/properties/Properties/properties/bitDepth",
          "type": "integer",
          "enum": [
            8,
            10,
            12,
            16
          ],
          "title": "Bitdepth",
          "description": "Bitdepth of the signal",
          "default": 8
        },
        "frameRate": {
          "$id": "#/properties/Properties/properties/frameRate",
          "type": "number",
          "title": "Frame Rate",
          "description": "provides the frame rate of the sequence",
          "default": 30,
          "examples": [
            24,
            25,
            30,
            60,
            90,
            120
          ]
        },
        "colourPrimaries": {
          "$id": "#/properties/Properties/properties/colourPrimaries",
          "type": "integer",
          "minimum": 1,
          "maximum": 12,
          "title": "Colour Primaries",
          "description": "ColourPrimaries according to ITU-T H.273.",
          "default": 1,
          "examples": [
            1,
            9
          ]
        },
        "transferCharacteristics": {
          "$id": "#/properties/Properties/properties/transferCharacteristics",
          "type": "integer",
          "minimum": 1,
          "maximum": 18,
          "title": "TransferCharacteristics",
          "description": "TransferCharacteristics according to ITU-T H.273.",
          "default": 1,
          "examples": [
            1,
            14,
            16,
            18
          ]
        },
        "matrixCoefficients": {
          "$id": "#/properties/Properties/properties/matrixCoefficients",
          "type": "integer",
          "minimum": 1,
          "maximum": 12,
          "title": "Matrix Coefficients",
          "description": "MatrixCoefficients according to ITU-T H.273.",
          "default": 1,
          "examples": [
            1,
            9
          ]
        },
        "sampleAspectRatio": {
          "type": "integer",
          "minimum": 1,
          "maximum": 16,
          "title": "SampleAspectRatio",
          "description": "SampleAspectRatio according to ITU-T H.273.",
          "default": 1,
          "examples": [
            1
          ]
        },
        "duration": {
          "$id": "#/properties/Properties/properties/duration",
          "type": "number",
          "title": "Duration",
          "description": "Duration of the sequence in seconds",
          "examples": [
            10
          ]
        },
        "frameCount": {
          "$id": "#/properties/Properties/properties/frameCount",
          "type": "integer",
          "title": "Total number of frames",
          "description": "Total number of frames in the sequence.",
          "default": 0,
          "examples": [
            300
          ]
        }
      },
      "additionalProperties": true
    },
    "copyRight": {
      "$id": "#/properties/copyRight",
      "type": "string",
      "title": "Copyright statement",
      "description": "Provides a copyright statement",
      "default": "",
      "examples": [
        "This material has been recorded by the Thomas Stockhammer, who owns the copyright. Thomas Stockhammer (licensor) has licensed it under the Creative Commons Attribution 4.0 license (https://creativecommons.org/licenses/by/4.0/). In short, this means you can freely reuse and distribute this content, also commercially, for as long you provide a proper attribution."
      ]
    },
    "Contact": {
      "$id": "#/properties/Contact",
      "type": "object",
      "title": "The Contact schema",
      "description": "An explanation about the purpose of this instance.",
      "default": {},
      "examples": [
        {
          "Name": "Thomas Stockhammer",
          "Company": "Qualcomm",
          "e-mail": "tsto@qti.qualcomm.com"
        }
      ],
      "required": [
        "Name",
        "e-mail"
      ],
      "properties": {
        "Name": {
          "$id": "#/properties/Contact/properties/Name",
          "type": "string",
          "title": "Name",
          "description": "Provides the name of the contact person.",
          "default": "",
          "examples": [
            "Thomas Stockhammer"
          ]
        },
        "Company": {
          "$id": "#/properties/Contact/properties/Company",
          "type": "string",
          "title": "Company",
          "description": "Company of the contact.",
          "default": "",
          "examples": [
            "Qualcomm"
          ]
        },
        "e-mail": {
          "$id": "#/properties/Contact/properties/e-mail",
          "type": "string",
          "format": "email",
          "title": "E-mail",
          "description": "E-mail address of contact person.",
          "default": "",
          "examples": [
            "tsto@qti.qualcomm.com"
          ]
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
